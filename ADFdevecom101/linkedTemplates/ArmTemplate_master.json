{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFdevecom101"
		},
		"DS_Parquet_fromRest_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DS_Parquet_fromRest'"
		},
		"LS_Manualfile_ToParquet_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_Manualfile_ToParquet'"
		},
		"LS_Parquet_fromcsv_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_Parquet_fromcsv'"
		},
		"DS_Parquet_fromRest_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlssidevecom101.dfs.core.windows.net/"
		},
		"LS_Manualfile_ToParquet_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsrawecom101.dfs.core.windows.net/"
		},
		"LS_Parquet_fromcsv_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlssidevecom101.dfs.core.windows.net/"
		},
		"LS_RestApi_toParquet_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://retoolapi.dev/smdCCy/data"
		},
		"Event_trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/09f1eb5a-de65-4127-81c0-3d2c66734c3e/resourceGroups/rg_devecom/providers/Microsoft.Storage/storageAccounts/adlsrawecom101"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFdevecom101_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DS_Parquet_fromRest_accountKey": {
						"value": "[parameters('DS_Parquet_fromRest_accountKey')]"
					},
					"LS_Manualfile_ToParquet_accountKey": {
						"value": "[parameters('LS_Manualfile_ToParquet_accountKey')]"
					},
					"LS_Parquet_fromcsv_accountKey": {
						"value": "[parameters('LS_Parquet_fromcsv_accountKey')]"
					},
					"DS_Parquet_fromRest_properties_typeProperties_url": {
						"value": "[parameters('DS_Parquet_fromRest_properties_typeProperties_url')]"
					},
					"LS_Manualfile_ToParquet_properties_typeProperties_url": {
						"value": "[parameters('LS_Manualfile_ToParquet_properties_typeProperties_url')]"
					},
					"LS_Parquet_fromcsv_properties_typeProperties_url": {
						"value": "[parameters('LS_Parquet_fromcsv_properties_typeProperties_url')]"
					},
					"LS_RestApi_toParquet_properties_typeProperties_url": {
						"value": "[parameters('LS_RestApi_toParquet_properties_typeProperties_url')]"
					},
					"Event_trigger_properties_typeProperties_scope": {
						"value": "[parameters('Event_trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}